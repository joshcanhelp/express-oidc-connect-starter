<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Expenses - Express OIDC Connect Starter</title>
</head>
<body>
<h1>Expenses Report</h1>
<a href="/">Home</a>
<br>
<table border="1">
  <tr>
    <% Object.keys(expenses[0]).forEach(k => { %>
      <th><strong><%= k %></strong></th>
    <% }) %>
  </tr>
  <% expenses.forEach(expense => { %>
    <tr>
      <% Object.keys(expense).forEach(k => { %>
        <td><%= expense[k] %></td>
      <% }) %>
    </tr>
  <% }) %>
</table>
</body>
</html>
